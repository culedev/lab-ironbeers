<div>
  <img src='{{beer.image_url}}' alt='beer' />
  <div>
    <h3>{{beer.name}}</h3>
    <h4>{{beer.tagline}}</h4>
    <p>{{beer.description}}</p>

    <ul><h4>Food Pairing</h4>
      {{#each beer.food_pairing}}
        <li>{{this}}</li>
      {{/each}}
    </ul>

    <span><strong>Brewers Tips:</strong> {{beer.brewers_tips}}</span>
  </div>

</div>